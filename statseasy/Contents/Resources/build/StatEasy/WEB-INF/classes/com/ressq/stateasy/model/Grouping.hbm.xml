<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.ressq.stateasy.model">
	<class name="Grouping" table="groupings">
		<id name="id" column="grouping" unsaved-value="null">
			<generator class="native"/>
		</id>
	
		<property name="uuid" index="groupinguuidindex">
			<column name="uuid"/>
		</property>
		
		<property name="name">
			<column name="name"/>
		</property>
	
		<property name="groupingData">
			<column name="groupingData"/>
		</property>
	
		<property name="defaultGrouping">
			<column name="defaultGrouping"/>
		</property>
		
		<many-to-one name="sport" column="sport" class="Sport" not-null="true" />
		
	</class>
</hibernate-mapping>