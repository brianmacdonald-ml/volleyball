<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.ressq.stateasy.model">
	<class name="PlaylistEntry" table="playlistEntries" batch-size="25">
		<id name="id" column="playlistEntry" unsaved-value="null">
			<generator class="native"/>
		</id>
		
		<property name="index">
			<column name="entryindex"/>
		</property>
		
		<many-to-one name="targetStat" column="targetStat" class="Stat" not-null="true" />
		
		<many-to-one name="targetVideo" column="targetVideo" class="GameVideo" not-null="true"/>
		
		<property name="startTime">
			<column name="starttime"/>
		</property>
		
		<property name="time">
			<column name="time"/>
		</property>
		
		<property name="endTime">
			<column name="endtime"/>
		</property>
		
	</class>
</hibernate-mapping>