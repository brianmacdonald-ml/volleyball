<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.ressq.stateasy.model">
	<class name="LocationData" table="locationdata">
		<id name="id" column="locationdata" unsaved-value="null">
			<generator class="native"/>
		</id>
		
		<property name="uuid" index="locationdatauuidindex">
			<column name="uuid"/>
		</property>
	
		<property name="x">
			<column name="x"/>
		</property>
	
		<property name="y">
			<column name="y"/>
		</property>
	
		<property name="startPoint">
			<column name="startPoint"/>
		</property>
	
		<property name="endPoint">
			<column name="endPoint"/>
		</property>
		
	</class>
</hibernate-mapping>