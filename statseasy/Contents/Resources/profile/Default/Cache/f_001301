<script type="text/x-jquery-tmpl" id="playerToAddTemplate">
	<tr id="ac_row_${id}">
		<td><div class="rowContainer"><input  id="ac_radio_${id}" 
					firstname="${firstName}" 
					lastname="${lastName}" 
					playerid="${id}" 
					type="radio" 
					name="player_radio" 
					value="${name}" 
					tabindex="-1"/> ${name}
		</div></td>
	</tr>
</script>

